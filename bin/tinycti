#!/usr/bin/env python3
"""
TinyCTI - Entry point script
"""

import os
import sys
from pathlib import Path

# Add the TinyCTI directory to the path
tinycti_dir = Path(__file__).parent.parent
sys.path.insert(0, str(tinycti_dir))

# Import and run TinyCTI
if __name__ == "__main__":
    from tinycti import main
    main()